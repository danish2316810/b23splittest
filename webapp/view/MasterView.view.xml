<mvc:View controllerName="app.splitapp.controller.MasterView"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page title="List View">
		<headerContent>
				<Button icon="sap-icon://sort" press="onSort"></Button>
			<Button icon="sap-icon://action" tooltip="Share" 
			press="onDetailView"/>
			
		
		</headerContent>
		<subHeader>
			<OverflowToolbar>
				<SearchField search="onSeach" liveChange="onSeach"/>
			</OverflowToolbar>
		</subHeader>
		<content>
			<List items="{ToolModel>/toolListSet}" id="idListCtrl" mode="SingleSelectMaster" selectionChange="onItemSelect">
				<items>
					<ObjectListItem icon="{ToolModel>image}" title="{ToolModel>toolsName}"
					intro="{ToolModel>uses}" number="{ToolModel>price} {ToolModel>unit}" numberState="Error"
					numberUnit="{ToolModel>unit}">
						<attributes>
							<ObjectAttribute text="{ToolModel>weight}" title="weight"></ObjectAttribute>
							<ObjectAttribute text="{ToolModel>warranty}" title="warranty"></ObjectAttribute>
						</attributes>
						<firstStatus>
								<ObjectStatus text="{ToolModel>availability}" title="availability" state="Error"></ObjectStatus>
							</firstStatus>
					</ObjectListItem>
					
					
					<!--<StandardListItem-->
					<!--info="for cutting walls" infoState="Error" title="Angle Grinder"></StandardListItem>-->
					<!--<CustomListItem>-->
					<!--	<content>-->
					<!--		<Label text="Angle Grinder"></Label>-->
					<!--		<Input width="200px"></Input>-->
					<!--	</content>-->
					<!--</CustomListItem>-->
					<!--<DisplayListItem label="angle grinder" value="Rs.2000"></DisplayListItem>-->
				 <!--  <FeedListItem icon="data:image/jpeg;base64,"-->
				 <!--  info="congrats on you upcoming moview" text="much awaited movie. precisely after 5-years " sender="karan"></FeedListItem>-->





				</items>
			</List>

		</content>
			<footer>
			<OverflowToolbar>
				<ToolbarSpacer/>
				<Button text="Accept" type="Accept" />
				<Button text="Reject" type="Reject" />
				<Button text="Edit" />
				<Button text="Delete" />
			</OverflowToolbar>
		</footer>
	</Page>
</mvc:View>